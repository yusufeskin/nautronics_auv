<link name="right_camera_link">
  <pose>0.18 -0.06 0 0 1.57 0</pose>  <!-- Y ekseninde -0.06m (sağda), rotasyon 0 -->
  <visual name="right_camera_visual">
    <geometry>
      <box>
        <size>0.03 0.03 0.03</size>
      </box>
    </geometry>
    <material>
      <ambient>1 0 0 1</ambient>
