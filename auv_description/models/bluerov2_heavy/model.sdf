    <camera>
      <horizontal_fov>1.047</horizontal_fov>  <!-- 60 derece -->
      <image>
        <width>640</width>
        <height>480</height>
        <format>RGB_INT8</format>
      </image>
      <clip>
        <near>0.1</near>
        <far>100</far>
