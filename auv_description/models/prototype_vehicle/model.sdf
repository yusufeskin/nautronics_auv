        </inertial>
        <visual name="base_link_visual">
          <geometry>
            <mesh>
              <uri>model://prototype_vehicle/meshes/sim.obj</uri>
              <scale>1 1 1</scale>
            </mesh>
          </geometry>
        </visual>
        <collision name="base_link_collision">
          <geometry>
            <mesh>
              <uri>model://prototype_vehicle/meshes/sim.obj</uri>
              <scale>1 1 1</scale>
            </mesh>
          </geometry>
