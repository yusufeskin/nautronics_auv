    <model name="prototype">



      <link name="base_link">
        <inertial>
          <pose>0.0 0.0 -0.05 0 0 0</pose>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.201</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.223</iyy>
            <iyz>0</iyz>
            <izz>0.269</izz>
